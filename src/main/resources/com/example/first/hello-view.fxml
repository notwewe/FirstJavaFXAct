<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pnLogin" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.first.HelloController">
   <children>
      <VBox alignment="CENTER" prefHeight="384.0" prefWidth="433.0" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
         <children>
            <GridPane prefHeight="243.0" prefWidth="393.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="68.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="168.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="175.0" minWidth="10.0" prefWidth="156.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label alignment="CENTER" prefHeight="36.0" prefWidth="117.0" text="Welcome!" textAlignment="CENTER" GridPane.columnIndex="1">
                     <font>
                        <Font name="Arial Bold" size="22.0" />
                     </font></Label>
                  <Label fx:id="userName" prefHeight="27.0" prefWidth="96.0" text="Username:" GridPane.rowIndex="1" />
                  <Label fx:id="Password" text="Password:" GridPane.rowIndex="2" />
                  <TextField fx:id="UsernameField" prefHeight="25.0" prefWidth="173.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="PasswordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Button fx:id="Loginbtn" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#onHelloButtonClick" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="welcomeText" prefHeight="34.0" prefWidth="227.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
